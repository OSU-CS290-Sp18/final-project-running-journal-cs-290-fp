<button type="button" id="create-log">
  <i class="fa fa-edit"></i>
</button>

<button type="button" id="clear-log">
  <i class="fa fa-window-close"></i>
</button>


<div id="modal-backdrop" class="hidden"></div>
<div id="create-log-modal" class="hidden">
  <div class="modal-dialog">

    <div class="modal-header">
      <h3>Record a Log</h3>
      <button type="button" class="modal-close-button">&times;</button>
    </div>

    <div class="modal-body">
      <div class="log-input-element">
        <label for="log-text-input">Title</label>
        <input type="text" id="log-attribution-input">
      </div>

      <div class = "log-input-element">
          <label for = "log-date-input">Date</label>
          <input type = "text" id="log-date-input">
      </div>

      <div class="log-input-element">
        <label for="log-text-input">Entry</label>
        <textarea id="log-text-input"></textarea>
      </div>

      <div class="log-input-element">
        <label for="log-miles-input">Miles</label>
        <input type="text" id="log-miles-input">
      </div>

    </div>

    <div class="modal-footer">
      <button type="button" class="modal-cancel-button">Cancel</button>
      <button type="button" class="modal-accept-button">Create Log</button>
    </div>

  </div>
</div>

<div id="clear-log-modal" class="hidden">
  <div class="modal-dialog">

    <div class="modal-header">
      <h3>Delete all entries?</h3>
      <button type="button" class="modal-close-button">&times;</button>
    </div>

    <div class="modal-footer">
      <button type="button" class="modal-cancel-button">No</button>
      <button type="button" class="modal-accept-button">Yes</button>
    </div>

  </div>
</div>

